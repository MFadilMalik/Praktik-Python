    "bmi = berat / (tinggi**2)\n",
    "if bmi > 22.9:\n",
    "    status = \"overweight\"\n",
    "elif bmi > 18.5:\n",
    "    status = \"normal\"\n",
    "else:\n",
    "    status = \"underweight\"\n",
    "print(\"Status BMI anda :\",status)"
